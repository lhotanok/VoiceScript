{
  "metadata": [
    {
      "src": "src/VoiceScript/**.cs",
      "dest": "api/api-voice-script"
    },
    {
      "src": "src/VoiceTranscription/**.cs",
      "dest": "api/api-voice-transcript"
    },
    {
      "src": "src/DiagramModel/**.cs",
      "dest": "api/api-diagram"
    },
    {
      "src": "src/CodeGeneration/**.cs",
      "dest": "api/api-code-gen"
    },
  ],
  "build": {
    "content": [
      {
        "files": [
          "api/**.yml"
        ]
      },
      {
        "files": [
          "articles/**.md",
          "articles/**/toc.yml",
          "toc.yml",
          "*.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**",
          "logo.svg",
          "favicon.ico"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "apidoc/**.md"
        ],
        "exclude": [
          "obj/**"
        ]
      }
    ],
    "dest": "docs",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "default",
      "templates/unity" 
    ],
    "postProcessors": [],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false
  }
}